{
    "name": "silbinarywolf/silverstripe-phpstan",
    "description": "PHPStan for Silverstripe",
    "type": "library",
    "keywords": [
        "php",
        "silverstripe",
        "static",
        "analysis",
        "phpstan",
        "scrutinizer"
    ],
    "license": "BSD-3-Clause",
    "authors": [
    {
        "name": "Jake Bentvelzen",
        "email": "jake@symbiote.com.au"
    }],
    "require": {
        "php": "~7.0",
        "silverstripe/framework": "~3.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "silverstripe/cms": "~3.1",
        "phpstan/phpstan": "0.9.2",
        "phpstan/phpstan-phpunit": "0.9.4",
        "phpunit/phpunit": "^7.0"
    },
    "scripts": {
        "phpcbf": "phpcbf -n src/ tests/"
    },
    "suggest": {
        "phpstan/phpstan-shim": "~0.9.0"
    },
    "autoload": {
        "psr-4": {
            "SilbinaryWolf\\SilverstripePHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilbinaryWolf\\SilverstripePHPStan\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
